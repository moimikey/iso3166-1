{
  "name": "iso3166-1",
  "version": "0.2.8",
  "description": "Tiny, fast, modular ISO 3166-1 alpha-2/alpha-3 parser.",
  "main": "index.js",
  "flat": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/moimikey/iso3166-1.git"
  },
  "keywords": [
    "iso3166",
    "country",
    "countries",
    "iso",
    "locale"
  ],
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com> (http://michael.hertzberg.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moimikey/iso3166-1/issues"
  },
  "homepage": "https://github.com/moimikey/iso3166-1",
  "devDependencies": {
    "brfs": "1.4.3",
    "browserify": "14.1.0",
    "csv2": "0.1.1",
    "event-stream": "3.3.4",
    "file-size": "1.0.0",
    "harmonize": "2.0.0",
    "hyperquest": "2.1.2",
    "jest-cli": "19.0.2",
    "jsonstream2": "1.1.1",
    "through2": "2.0.3",
    "uglifyjs": "2.4.10"
  },
  "dependencies": {
    "zlib-browserify": "0.0.3"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "browser": {
    "fs": false
  },
  "scripts": {
    "build-basic": "echo 'Building build/iso3166-1.js...' && browserify index.js &> build/iso3166-1.js",
    "build-global": "echo 'Building build/iso3166-1.global.js...' && browserify index.js --standalone iso3166 &> build/iso3166-1.global.js",
    "build-ugly": "echo 'Building build/iso3166-1.min.js...' && cat build/iso3166-1.js | uglifyjs --screw-ie8 -c -m &> build/iso3166-1.min.js",
    "build-ugly-global": "echo 'Building build/iso3166-1.global.min.js...' && cat build/iso3166-1.global.js | uglifyjs --screw-ie8 -c -m &> build/iso3166-1.global.min.js",
    "build": "echo 'Building iso3166-1...' && npm run build-basic && npm run build-global && npm run build-ugly && npm run build-ugly-global",
    "generate": "bin/generate",
    "test": "jest"
  },
  "jest": {
    "rootDir": "src",
    "testPathIgnorePatterns": [
      "/__tests__/[^/]*/.+",
      "node_modules"
    ]
  },
  "resolutions": {
    "through2": "2.0.3",
    "defined": "1.0.0",
    "quote-stream": "1.0.2",
    "resolve": "1.3.2",
    "minimist": "1.2.0",
    "concat-stream": "1.5.2",
    "duplexer2": "0.1.4",
    "escodegen": "1.8.1",
    "readable-stream": "2.2.3",
    "inherits": "2.0.3",
    "isarray": "1.0.0",
    "esutils": "2.0.2",
    "estraverse": "1.9.3",
    "esprima": "3.1.3",
    "source-map": "0.5.6",
    "acorn": "4.0.11",
    "object-keys": "1.0.11",
    "xtend": "4.0.1",
    "punycode": "1.4.1",
    "jsonparse": "1.3.0",
    "convert-source-map": "1.4.0",
    "json-stable-stringify": "1.0.1",
    "yargs": "6.6.0",
    "ansi-styles": "3.0.0",
    "supports-color": "3.2.3",
    "async": "2.1.5",
    "strip-bom": "3.0.0",
    "wordwrap": "1.0.0",
    "camelcase": "3.0.0",
    "cliui": "3.2.0",
    "webidl-conversions": "4.0.1",
    "assert-plus": "1.0.0",
    "fb-watchman": "2.0.0",
    "bser": "2.0.0",
    "find-up": "2.1.0",
    "path-exists": "3.0.0"
  }
}
